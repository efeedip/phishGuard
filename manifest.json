{
    "manifest_version": 3,
    "name": "PhishGuard",
    "description": "This extention allows you to asses phishing risks of the web site you visit.",
    "version": "0.0.0.1",
    "permissions": ["activeTab","storage","tabs","webNavigation","notifications"],
    "action": {
      "default_popup": "popup.html"
    },
    "icons": {
      "16": "images/icon-16-3.png",
      "48": "images/icon-48-3.png",
      "128": "images/icon-128-3.png",
      "256": "images/icon-248-3.png"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ],
    "background": {
      "service_worker": "background.js"
    }
  }
  