{
    "manifest_version": 3,
    "name": "Phishing Detection",
    "description": "This extention allows you to asses phishing risks of the web site you visit.",
    "version": "1.0",
    "permissions": ["activeTab","storage","tabs","webNavigation","notifications"],
    "action": {
      "default_popup": "popup.html"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ],
    "background": {
      "service_worker": "background.js"
    }
  }
  