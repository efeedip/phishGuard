{
    "manifest_version": 3,
    "name": "Source Code Viewer",
    "version": "1.0",
    "description": "View the source code of a website.",
    "permissions": ["activeTab"],
    "action": {
      "default_icon": {
        "16": "images/icon-16.png",
        "48": "images/icon-48.png",
        "128": "images/icon-128.png"
      }
    },
    "icons": {
      "16": "images/icon-16.png",
      "48": "images/icon-48.png",
      "128": "images/icon-128.png"
    },
    "content_scripts": [
        {
          "matches": ["<all_urls>"],
          "js": ["content.js"]
        }
      ]
  }
  