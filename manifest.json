{
    "manifest_version": 3,
    "name": "WHOIS Extension",
    "version": "1.0",
    "permissions": [  "activeTab","storage","tabs","webNavigation"],
    "action": {
      "default_popup": "popup.html"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ],
    "background": {
      "service_worker": "background.js"
    }
  }
  